<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covinfo_19">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_app_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        
        <activity android:name=".comparativa_regiones.Fallecidos" />
        <activity android:name=".comparativa_regiones.CasosActivosConfirmados" />
        <activity android:name=".comparativa_regiones.CasosNuevos" />
        <activity android:name=".comparativa_regiones.CasosAcumulados" />
        <activity android:name=".ReporteDeUnaRegion" />
        <activity android:name=".ReporteDiario" />
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>